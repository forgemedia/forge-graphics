doctype html
html(lang='en')
    head
        title #{project} Graphics Dashboard
        meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
        link(rel='stylesheet', href='/css/dashboard.css')
    body
        header: ._container
            ._col_2._col_2of3_im: h1 Forge Graphics Dashboard
            ._col_2._col_3_im.align-right: h1: small= project
        ._container#dashboard
            ._col_5#sidebar: ul
                li.active: a(href='#') Active menu item
                each id in [...Array(5).keys()]
                    li: a(href='#') Menu item
            ._col_4of5#panel
                .header-panel: h1 abc
                #bugPanel
                    toggle-button(v-model='dataStore.showBug', title='Show Bug')

        script(src='/jspm_packages/system.js')
        script(src='/js/config.js')
        script System.import('/js/dash.js')
